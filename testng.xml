<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Swaglabs Test Suite" parallel="tests">
  <test thread-count="2" name="Login Page Test">
    <groups>
      <run>
        <include name="Login Errors" />
        <include name="Login Test" />
      </run>
    </groups>
    <classes>
      <class name="com.swaglabs.swaglabs.POM.test.TestSwagLabsLoginPage" />
    </classes>
  </test> <!-- Login Page Tests -->
  <test name="Product Page Test">
    <classes>
      <class name="com.swaglabs.swaglabs.POM.test.TestSwagLabsProductPage"></class>
    </classes>
  </test>
  <test name="Logout from Application">
    <classes>
      <class name="com.swaglabs.swaglabs.Keyword.TestSwagLabsLogout" />
    </classes>
  </test>
  <test name="Add Product to Shopping Cart and Verify">
    <classes>
      <class name="com.swaglabs.swaglabs.Keyword.TestSwagLabsInventory" />
    </classes>
  </test>
</suite> <!-- Suite -->
